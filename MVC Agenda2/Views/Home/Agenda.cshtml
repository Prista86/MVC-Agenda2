@{
    ViewData["Title"] = "Agenda";
}
<h1>@ViewData["Title"]</h1>

@model List<PersonModel>;

<table>
    <tr>
        <td> Nome</td>
        <td> Cognome</td>
        <td> Indirizzo Email</td>
    </tr>
    @foreach (PersonModel p in Model)
    {
        <tr>
            <td>@p.Nome</td>
            <td>@p.Cognome</td>
            <td>@p.IndirizzoEmail</td>
            @*<td>@p.NumeroCellulare</td>*@
        </tr>
    }
</table>

